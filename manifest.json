{
    "manifest_version": 3,
    "name": "SisAE Plus",
    "version": "1.0",
    "description": "Extensão para aprimorar a experiência do usuário no SisAE, com funcionalidades adicionais e interface otimizada.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["https://sisae.ifce.edu.br/*"],
    "action": {
      "default_popup": "popup.html",
        "default_icon": {
          "16": "./src/assets/ifce.png",
          "24": "./src/assets/ifce.png",
          "32": "./src/assets/ifce.png",
          "48": "./src/assets/ifce.png",
          "128": "./src/assets/ifce.png"
        }
    },
    "icons": {
      "16": "./src/assets/ifce.png",
      "24": "./src/assets/ifce.png",
      "32": "./src/assets/ifce.png",
      "48": "./src/assets/ifce.png",
      "128": "./src/assets/ifce.png"
    }
}